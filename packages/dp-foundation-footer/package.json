{
  "name": "@greatminds/dp-foundation-footer",
  "version": "0.0.0-alpha.1",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build-app": "cross-env BABEL_ENV=production babel ./src --out-dir dist --extensions .js,.ts,.tsx --ignore src/**/*.stories.tsx",
    "build-types": "tsc --project tsconfig.types.json --jsx react-jsx",
    "build": "npm run build-types && npm run build-app",
    "build-clean": "rm -rf dist",
    "husky": "npm run format && npm run lint",
    "format": "pretty-quick --staged",
    "lint": "eslint -c .eslintrc.js --ext .ts,.tsx ./src --fix",
    "lint:check": "eslint -c .eslintrc.js --ext .ts,.tsx ./src",
    "lint:ci:check": "eslint -c .eslintrc.js --ext .ts,.tsx ./src --quiet",
    "lint:fix": "eslint -c .eslintrc.js --ext .ts,.tsx ./src --fix",
    "test": "jest --no-watch",
    "test:cov": "npm run test -- --coverage",
    "test:watch": "npm run test -- --coverage --watch",
    "test:ci": "jest --ci --forceExit --reporters=jest-html-reporter",
    "test:ci:cov": "jest --ci --reporters=jest-html-reporter --coverageReporters=lcov --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run husky"
    }
  },
